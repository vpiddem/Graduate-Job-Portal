<h1>Listing Applicants</h1>
<table>
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>User ID</th>
    <th>Phone Number</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @job_seekers.each do |jobseeker| %>
      <tr>
        <td><%= jobseeker.first_name %></td>
        <td><%= jobseeker.last_name %></td>
        <td><%= jobseeker.user_id %></td>
        <td><%= jobseeker.phone_number %></td>
      </tr>
  <% end %>
  </tbody>
</table>